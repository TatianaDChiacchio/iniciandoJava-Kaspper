package PagandoImpostos;

public class Principal {

	public static void main(String[] args) {
		
		Pessoa pf1 = new PessoaFisica("Diego", 50000,1000);
		Pessoa pf2 = new PessoaFisica("Euler",80000,1500);
		
		System.out.printf("%s%s%.2f%n",pf1.getNome()," vai pagar o valor de: ", pf1.calcularImposto());
		System.out.printf("%s%s%.2f%n",pf2.getNome()," vai pagar o valor de: ", pf2.calcularImposto());
	}

}
